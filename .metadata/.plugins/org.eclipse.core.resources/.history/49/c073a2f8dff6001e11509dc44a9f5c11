package Exercicios;

public class Exercicio12 {

	public static void main(String[] args) {
		// 12. Usando o operador ternário, crie um programa que recebe dois números inteiros como entrada e exibe o
		//maior deles.
		//começando com 
		
		
		int numeroDias = 2 ;
		System.out.println(numeroDias <= 15; ? “Primeira quinzena” : “Segunda quinzena”);
		
	}

}
